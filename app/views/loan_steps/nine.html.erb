<div class="row-fluid">
	<div class="span9">
		<div class="well">
			<p class="lead text-center"><legend><strong>放款</strong></legend></p>
			<%= simple_form_for @custloan,:url => wizard_path, :method => :put, :html => { :class => 'form-horizontal' } do |f| %>
				<%= render 'nine_fields', :f => f %>
				<div class="form-actions">
					<% if @custloan.guarantee_type == get_dict("guaranteeType", 1).id %>
					<%= link_to '上一步', previous_wizard_path, :class => 'btn' %>
					<% else %>
					<%= link_to '上一步', wizard_path(:eight), :class => 'btn' %>
					<% end %>
					<%= f.submit '完成', :class => 'btn btn-primary' %>
				</div>
			<% end %>
		</div>
	</div>
	<div class="span3">
		<%= render 'loan_steps/helps/nine_helps' %>
	</div>
</div>