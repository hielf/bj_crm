<div class="row-fluid">
	<div class="span9">
		<div class="well">
			<p class="lead text-center"><legend><strong>是否有其他账户</strong></legend></p>
				<%= simple_form_for @custloan,:url => wizard_path, :method => :put, :html => { :class => 'form-horizontal' } do |f| %>
			  	<%= render 'seven_three_fields', :f => f %>
					<div class="form-actions">
						<%= link_to '上一步', previous_wizard_path, :class => 'btn' %>
						<%= link_to '无，跳过', wizard_path(:eight), :class => 'btn btn-danger' %>
						<%= f.submit '有，下一步', :class => 'btn btn-primary' %>
					</div>
				<% end %>
		</div>
	</div>
	<div class="span3">
		<%= render 'loan_steps/helps/seven_helps' %>
	</div>
</div>