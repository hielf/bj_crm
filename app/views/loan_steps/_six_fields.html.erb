<fieldset>
<%= f.input :current_step, as: :hidden, input_html: { value: 6 } %>

<%= f.simple_fields_for :loanstepsixths do |p| %>
	<%= p.input :cust_id, as: :hidden, input_html: { value: @cust.id } %>
	<%= p.input :contract_type, :collection => get_dict_by_type('contractType'), 
							:label => '选择合同类型', required: true %>
	<%= p.input :if_notarization, as: :select, :label => '是否公证' %>
<% end %>
</fieldset>