<div class="row-fluid">
	<div class="span9">
		<div class="well">
			<p class="lead text-center"><legend><strong>银行审批结果</strong></legend></p>
				<%= simple_form_for @custloan,:url => wizard_path, :method => :put, :html => { :class => 'form-horizontal' } do |f| %>
						<%= render 'five_fields', :f => f %>
					<div class="form-actions">
						<%= link_to '上一步', previous_wizard_path, :class => 'btn' %>
						<%= f.submit '确认', :class => 'btn btn-primary' %>
					</div>
				<% end %>
		</div>
	</div>
	<div class="span3">
		<%= render 'loan_steps/helps/five_helps' %>
	</div>
</div>