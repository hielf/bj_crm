<fieldset>
	<div class="row-fluid">
		<div class="span12">
			<%= f.input :cust_type, :collection => get_dict_by_type("reportcustType"), :label => '客户类型', :include_blank => false %>
		</div>
	</div>
	<div class="row-fluid">
		<div class="span12">
			<%= f.input :cust_region, :collection => get_dict_by_type("custRegion"), :label => '所在地域', :include_blank => false %>
		</div>
	</div>
	<div class="row-fluid">
		<div class="span6">
			<%= f.input :company_name, :label => '公司名称' %>
		</div>
		<div class="span6">
			<%= f.input :boss, :label => '负责人' %>
		</div>
	</div>
	<div class="row-fluid">
		<div class="span6">
			<%= f.input :contact, :label => '联系方式' %>
		</div>
		<div class="span6">
			<%= f.input :address, :label => '经营地址' %>
		</div>
	</div>
	<div class="row-fluid">
		<div class="span12">
			<%= f.input :financial, :label => '财务负责人' %>
		</div>
	</div>
		  <%= f.hidden_field :_destroy %>
		  <%= link_to "删除", '#', class: "remove_fields" %>
</fieldset>