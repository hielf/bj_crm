<div class="row-fluid">
	<div class="span9">
		<div class="well">
			<p class="lead text-center"><legend><strong>放款</strong></legend></p>
			<%= simple_form_for @custloan, :html => { :class => 'form-horizontal' } do |f| %>
				<%= render 'loan_add_fields', :f => f %>
				<div class="form-actions">
					<%= f.submit '完成', :class => 'btn btn-primary' %>
				</div>
			<% end %>
		</div>
	</div>
	<div class="span3">
		<%= render 'loan_steps/helps/nine_helps' %>
	</div>
</div>